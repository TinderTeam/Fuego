<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
	
<hibernate-mapping
	package="cn.tinder.fuego.domain.po">
	<class name="RoleMapMenu" table="role_map_menu">
	    <composite-id>
	       <key-property name="role" column="ROLE" ></key-property>
           <key-property name="menuID" column="MENU_ID" ></key-property>
	    </composite-id>
	</class>
</hibernate-mapping>
 