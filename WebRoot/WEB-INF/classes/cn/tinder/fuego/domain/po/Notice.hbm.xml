<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="cn.tinder.fuego.domain.po">
 <class name="Notice" table="notice">
  <id column="TRANS_ID" name="transID" type="java.lang.String">
   <generator class="assigned"/>
  </id>
  <property column="NOTICE_TYPE" generated="never" lazy="false" name="noticeType"/>
  <property column="USER_NAME" generated="never" lazy="false" name="userName"/>
 </class>
</hibernate-mapping>
