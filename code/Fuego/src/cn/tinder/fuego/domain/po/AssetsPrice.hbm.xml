<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
	
<hibernate-mapping
	package="cn.tinder.fuego.domain.po">
	<class name="AssetsPrice" table="assets_price">
	
		  <composite-id>
	       
           <key-property name="assetsName" column="ASSETS_NAME" ></key-property>
           <key-property name="manufacture" column="MANUFACTURE"  ></key-property>
           <key-property name="spec" column="SPEC"  type="java.lang.String" ></key-property>

	    </composite-id>
	    
			<property name="price" column="PRICE" not-null="false"></property>
					
	</class>
</hibernate-mapping>
 