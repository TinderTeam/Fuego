<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
	
<hibernate-mapping
	package="cn.tinder.fuego.domain.po">
	<class name="TransEventType" table="trans_event_type">
	    <id name="type" column="TYPE"  type="java.lang.String" >
			<generator class="assigned"/>
		</id>
			<property name="step" column="STEP" not-null="false"></property>
			<property name="currentID" column="CURRENT_ID" not-null="false"></property>
			
			
	</class>
</hibernate-mapping>
 