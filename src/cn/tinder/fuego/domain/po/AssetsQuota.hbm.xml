<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
	
<hibernate-mapping
	package="cn.tinder.fuego.domain.po">
	<class name="AssetsQuota" table="assets_quota">
	    <id name="assetsName" column="ASSETS_NAME"  type="java.lang.String" >
			<generator class="assigned"/>
		</id>
			<property name="deptID" column="DEPT_ID" not-null="false"></property>
			<property name="spec" column="SPEC" not-null="false"></property>
			<property name="quantity" column="QUANTITY" not-null="false"></property>
			<property name="expectYear" column="EXPECT_YEAR" not-null="false"></property>
	
	</class>
</hibernate-mapping>
 