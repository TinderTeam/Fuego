<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
	
<hibernate-mapping
	package="cn.tinder.fuego.domain.po">
	<class name="AssetsPrice" table="assets_price">
	    <id name="spec" column="SPEC"  type="java.lang.String" >
			<generator class="assigned"/>
		</id>
			<property name="price" column="PRICE" not-null="false"></property>
					
	</class>
</hibernate-mapping>
 