<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
	
<hibernate-mapping
	package="cn.tinder.fuego.domain.po">
	<class name="PurchasePlan" table="purchase_plan">
	    <composite-id>
	       <key-property name="transID" column="TRANS_ID" ></key-property>
           <key-property name="assetsName" column="ASSETS_NAME" ></key-property>
           <key-property name="manufacture" column="MANUFACTURE"  ></key-property>
           <key-property name="spec" column="SPEC"  ></key-property>
           
	    </composite-id>
	   
  			
			<property name="unit" column="UNIT" not-null="false"></property>
			<property name="quantity" column="QUANTITY" not-null="false"></property>
			<property name="price" column="PRICE" not-null="false"></property>
			<property name="sum" column="SUM" not-null="false"></property>
			<property name="note" column="NOTE" not-null="false"></property>
				
	</class>
</hibernate-mapping>
 